//func var. parametr olaraq 1 array ve 1 callback func qaytarir.
//func gelen arraydeki ededleri 5 e vursun
//(parametrden gelen callback func-u ist etmekle)

function Multiplier(arr,callback) {

    let result=[];
    for (let i = 0; i < arr.length; i++) {
       result[i]=callback(arr[i])
    }
    return result;
}
function mycallback(value) {
    return value*5;
}
console.log(Multiplier([1,2,3],mycallback));
