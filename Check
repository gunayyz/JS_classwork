function result(deg)
{

    var txt = "";

    if( deg >= 85 && deg < 100){
      txt = "excellent";
    } else if (deg >= 75 && deg <=85){
       txt = "very good ";
    } else if ( deg >=65 && deg <= 75){
       txt = " good ";
    } else if (deg >= 50 && deg <= 65){
       txt = "fairly good";
    } else if (deg < 50 ){
       txt = "fail";
    }
    
    return txt;
  
}

        
result(95);
var degree_result = result(deg);

document.write(degree_result);




