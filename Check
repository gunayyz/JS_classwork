        var txt = "";
    if( arr.deg >= 95 && arr.deg < 100){
      txt = "yuksek seref";
    } 
    else if (arr.deg >= 85 && arr.deg <=94){
       txt = "seref";
    } 
    else if (arr.deg >=65 && arr.deg <= 84){
       txt = "adi";
    } 
    else if (arr.deg >= 0 &&arr.deg <= 64){
       txt = "kesr";
    return txt;
    }    
    }
}
